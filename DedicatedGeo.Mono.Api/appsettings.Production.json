{
  "App": {
    "Env": "Production"
  },
  "Mysql": {
    "ConnectionString": "Data Source=/Database/MyDatabase.db"
  },
  "NLog": {
    "rules": [
      {
        "ruleName": "JsonLogs",
        "logger": "*",
        "minLevel": "Warn",
        "writeTo": "jsonlog"
      },
      {
        "logger": "*",
        "minLevel": "Warn",
        "writeTo": "Console"
      }
    ]
  }
}
